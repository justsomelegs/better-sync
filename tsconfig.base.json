{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noUncheckedIndexedAccess": true,
    "useDefineForClassFields": true,
    "verbatimModuleSyntax": true,
    "lib": ["ES2021", "DOM" ],
    "types": []
  }
}


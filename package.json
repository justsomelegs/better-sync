{
  "name": "just-sync",
  "version": "0.0.0",
  "description": "Type-safe sync engine MVP (server/client) â€“ ESM-only",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./storage/server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs"
    },
    "./storage/client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs"
    },
    "./next-js": {
      "types": "./dist/next-js.d.ts",
      "import": "./dist/next-js.mjs"
    },
    "./adapters/drizzle": {
      "types": "./dist/adapters/drizzle.d.ts",
      "import": "./dist/adapters/drizzle.mjs"
    },
    "./adapters/prisma": {
      "types": "./dist/adapters/prisma.d.ts",
      "import": "./dist/adapters/prisma.mjs"
    },
    "./adapters/libsql": {
      "types": "./dist/adapters/libsql.d.ts",
      "import": "./dist/adapters/libsql.mjs"
    },
    "./adapters/postgres": {
      "types": "./dist/adapters/postgres.d.ts",
      "import": "./dist/adapters/postgres.mjs"
    }
  },
  "bin": {
    "just-sync": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "turbo": "turbo",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "bench": "node benchmarks/adapter-sqlite-insert.mjs && node benchmarks/server-mutate-insert.mjs && node benchmarks/server-select-window.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justsomelegs/better-sync.git"
  },
  "bugs": {
    "url": "https://github.com/justsomelegs/better-sync/issues"
  },
  "homepage": "https://github.com/justsomelegs/better-sync#readme",
  "keywords": [],
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^24.3.3",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.9",
    "drizzle-orm": "^0.36.0",
    "@libsql/client": "^0.6.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "better-call": "^1.0.18",
    "sql.js": "^1.10.3",
    "ulid": "^3.0.1",
    "zod": "^4.1.8",
    "@libsql/client": "^0.10.0",
    "pg": "^8.11.5"
  }
}